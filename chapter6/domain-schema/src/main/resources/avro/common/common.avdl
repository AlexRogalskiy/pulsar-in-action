@namespace("com.gottaeat.domain.common")

protocol CommonsProtocol {

  record Address {
    string street; 
    string city; 
    string state; 
    string zip;
  }
  
  record LatLon {
    double latitude;
    double longitude;
  }
  
  record GeoEncodedAddress {
  	Address addr;
  	LatLon geo;
  }
  
}